{
  "name": "perryment-web",
  "version": "1.0.0",
  "description": "",
  "main": "Brocfile.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean && broccoli build dist",
    "serve": "npm run build && broccoli serve",
    "deploy": "gh-pages-deploy",
    "clean": "rimraf dist",
    "clean-source": "rimraf Brocfile.js LICENSE package*.json *.code-workspace README.md src"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "cname": "perryment.kevinrpb.me",
    "prep": [
      "build",
      "clean-source"
    ],
    "noprompt": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinrpb/perryment-web.git"
  },
