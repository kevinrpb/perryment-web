  },
  "homepage": "https://github.com/kevinrpb/perryment-web#readme",
  "devDependencies": {
    "broccoli": "^3.4.1",
    "broccoli-eyeglass": "^6.0.0",
    "broccoli-funnel": "^3.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "gh-pages-deploy": "^0.5.1",
    "node-sass": "^4.13.1",
    "rimraf": "^3.0.2"
  }
